<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://java.sun.com/xml/ns/javaee" version="2.5">
    <display-name>Quick Servlet Demo Application</display-name>
<!---->

    <servlet>
        <servlet-name>greetServlet</servlet-name>
        <servlet-class>com.mnouswen.server.GreetingServiceImpl</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>greetServlet</servlet-name>
        <url-pattern>/MainStart/greet</url-pattern>
    </servlet-mapping>
    <welcome-file-list>
        <welcome-file>MainStart.html</welcome-file>
    </welcome-file-list>
</web-app>